//solution 1.
public class Main
{
	public static void main(String[] args) {
        
        for(int i=1;i<=7;i++){
            char a='A';
            for(int j=i;j<=7;j++){
                
                System.out.print(a+" ");
                a++;
            }
            System.out.println();

        }
	}
}

//solution 2.
public class Main
{
	public static void main(String[] args) {
                for(int i=1;i<=7;i++){
                    char ch='A';
                    for(int j=1;j<=8-i;j++){
                        System.out.print(ch+" ");
                        ch++;
                    }
                    System.out.println();
                }
                
	}
}

