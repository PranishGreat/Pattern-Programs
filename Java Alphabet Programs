22.
       A
     B A B
    C B A B C
   D C B A B C D
  E D C B A B C D E
 F E D C B A B C D E F
G F E D C B A B C D E F G

public class Main
{
	public static void main(String[] args) {
                char ch='A';
                char a=' ';
                char a1=' ';
                for(int i=1;i<=7;i++){
                    a=ch;
                    for(int j=1;j<=7-i+1;j++){
                        System.out.print(" ");
                    }
                    for(int k=1;k<=i;k++){
                        System.out.print(a+" ");
                        a--;
                    }
                    ch++;
                
                    a1='B';
                    for(int j=1;j<i;j++){
                        System.out.print(a1+" ");
                        a1++;
                    } 
                
                    System.out.println();   
            
            }
    }
}


