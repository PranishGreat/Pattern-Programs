public class Main
{
	public static void main(String[] args) {
         
        for(int i=1;i<=7;i++){
           
           for(int k=7;k>=i;k--){
               System.out.print(" ");
           }
           char a='A';
            for(int j=1;j<=i;j++){
                
                System.out.print(a+" ");
                a++;
            }
            System.out.println();

        }
	}
}


