D C B A
E D C B A
F E D C B A
G F E D C B A


public class Main
{
	public static void main(String[] args) {
            char a='A';    
            
        for(int i=1;i<=7;i++){
            char ch=a;
            for(int k=1;k<=i;k++){
                System.out.print(ch+" ");
                ch--;
            }
            a++;
        System.out.println();
        }
        
    }
}

9.
       A
      ABA
     ABCBA
    ABCDCBA
   ABCDEDCBA
  ABCDEFEDCBA
 ABCDEFGFEDCBA

public class Main
{
	public static void main(String[] args) {
                
                char a=' ';
                char a1=' ';
                for(int i=1;i<=7;i++){
                char ch='A';
                    a=ch;
                    for(int j=1;j<=7-i+1;j++){
                        System.out.print(" ");
                    }
                    for(int k=1;k<=i;k++){
                        System.out.print(a);
                        a++;
                    }
                    ch++;
                
                    a1='A';
                    for(int j=1;j<i;j++){
                        System.out.print(a1);
                        a1++;
                    } 
                
                    System.out.println();   
            
            }
    }
}


