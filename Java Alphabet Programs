public class Main
{
	public static void main(String[] args) {
        for(int i=1;i<=7;i++){
            char a='A';
            for(int j=1;j<=i*2-1;j++){
                System.out.print(a+" ");
                a++;
            }
            System.out.println();
        }
	}
}


