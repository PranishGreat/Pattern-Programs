public class Main
{
	public static void main(String[] args) {
    
        char ch=' ';
        for(int i=1;i<=7;i++){
        char  a='A';
            for(int k=1;k<i;k++){
                System.out.print(a+" ");
                a++;
            }
            for(int k=1;k<=i;k++){
                System.out.print(a+" ");
                a--;
            }
            System.out.println();
           }
           
	}
}

