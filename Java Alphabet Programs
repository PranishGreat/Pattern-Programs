public class Main
{
	public static void main(String[] args) {
        
        for(int i=7;i>=1;i--){
            char a='G';
            for(int j=7;j>=i;j--){
                
                System.out.print(a+" ");
                a--;
            }
            
            System.out.println();

        }
	}
}

