public class Main
{
	public static void main(String[] args) {
    
        char ch=' ';
        for(int i=1;i<=7;i++){
        char  a='A';
            for(int k=1;k<=i;k++){
                System.out.print(a+" ");
                a++;
            }
            System.out.println();
           }
        for(int i=7;i>=1;i--){
          char  a1='A';
            for(int k=i-1;k>=1;k--){
                System.out.print(a1+" ");
                a1++;
            }
            System.out.println();
           }
           
	}
}

20.
G F E D C B A
F E D C B A
E D C B A
D C B A
C B A
B A
A
A
B A
C B A
D C B A
E D C B A
F E D C B A
G F E D C B A

public class Main
{
	public static void main(String[] args) {
        char  a='G';
        char ch=' ';
        for(int i=7;i>=1;i--){
            ch=a;
            for(int k=i;k>=1;k--){
                System.out.print(ch);
                ch--;
            }
            a--;
            System.out.println();
           }
           
        char  a1='A';
        char ch1=' ';
        for(int i=1;i<=7;i++){
            ch1=a1;
            for(int k=1;k<=i;k++){
                System.out.print(ch1);
                ch1--;
            }
            a1++;
            System.out.println();
           }
	}
}


