        public class Main
{
	public static void main(String[] args) {
        char a='A';
        for(int i=1;i<=7;i++){
            for(int j=1;j<=7-i;j++){
                System.out.print(" ");
            }
            for(int k=i;k>=1;k--){
                System.out.print(a);
                
            }
            System.out.println();
            a++;
        }
	}
}

          
