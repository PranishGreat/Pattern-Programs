public class Main
{
	public static void main(String[] args) {
                char ch='G';
                char a=' ';
            for(int i=1;i<=7;i++){
               a=ch;
                 for(int j=1;j<=7-i;j++){
                    System.out.print(" ");
                }
                for(int k=1;k<=i;k++){
                    System.out.print(a+" ");
                    a++;
                }
                ch--;
                 System.out.println();   
            }
    }
}


