13.
A A A A A A A
A A A A A B B
A A A A C C C
A A A D D D D
A A E E E E E
A F F F F F F
G G G G G G G


public class Main
{
	public static void main(String[] args) {
          
     char a='A';         
        for(int i=1;i<=7;i++){
          
            for(int k=i;k<7;k++){
                System.out.print("A ");
            }
    
            for(int j=1;j<=i;j++){
                System.out.print(a+" ");
            }
            a++;  

        System.out.println();
        }
        
    }
}

14.
A B C D E F G F E D C B A
B C D E F G F E D C B
C D E F G F E D C
D E F G F E D
E F G F E
F G F
G

public class Main
{
	public static void main(String[] args) {
                 char ch='A';
                 char a=' ';
            for(int i=1;i<=7;i++){
                a=ch;
                for(int j=7;j>=i+1;j--){
                    System.out.print(a);
                    a++;
                }
                for(int j=7;j>=i;j--){
                    System.out.print(a);
                    a--;
                }
                ch++;
                System.out.println();
        }
                
	}
}
 
15.
       A
      A B
     A B C
    A B C D
   A B C D E
  A B C D E F
 A B C D E F G
  A B C D E F
   A B C D E
    A B C D
     A B C
      A B
       A

public class Main
{
	public static void main(String[] args) {
                
            for(int i=1;i<=7;i++){
                 char ch='A';
                for(int j=1;j<=7-i;j++){
                    System.out.print(" ");
                }
                for(int k=1;k<=i;k++){
                    System.out.print(ch+" ");
                    ch++;
                }
                 System.out.println();   
            }
            
            for(int i=1;i<=7;i++){
                char ch='A';
                for(int j=1;j<=i;j++){
                    System.out.print(" ");
                }
                for(int k=6;k>=i;k--){
                    System.out.print(ch+" ");
                    ch++;
                }
                System.out.println();
            }        
	}
}
  


  
16.
A B C D E F G
 B C D E F G
  C D E F G
   D E F G
    E F G
     F G
      G
      G
     F G
    E F G
   D E F G
  C D E F G
 B C D E F G
A B C D E F G

public class Main
{
	public static void main(String[] args) {
                char ch='A';
                char a=' ';
                char a1=' ';
            for(int i=1;i<=7;i++){
               a=ch;
                 for(int j=1;j<=i;j++){
                    System.out.print(" ");
                }
                for(int k=7;k>=i;k--){
                    System.out.print(a+" ");
                    a++;
                }
                ch++;
                 System.out.println();   
            }
                char g='G';
            for(int i=1;i<=7;i++){
               a1=g;
                 for(int j=1;j<=7-i+1;j++){
                    System.out.print(" ");
                }
                for(int k=1;k<=i;k++){
                    System.out.print(a1+" ");
                    a1++;
                }
                g--;
                 System.out.println();   
            }
    }
}



17.
ABCDEFG
   BCDEFG
      CDEFG
         DEFG
            EFG
               FG
                 G
                 G
               FG
            EFG
         DEFG
      CDEFG
   BCDEFG
ABCDEFG

public class Main
{
	public static void main(String[] args) {
                char ch='A';
                char a=' ';
                char a1=' ';
            for(int i=1;i<=7;i++){
               a=ch;
                 for(int j=7;j>=7-i+2;j--){
                    System.out.print("   ");
                }
                for(int k=7;k>=i;k--){
                    System.out.print(a);
                    a++;
                }
                ch++;
                 System.out.println();   
            }
                char g='G';
            for(int i=1;i<=7;i++){
               a1=g;
             
                for(int j=1;j<=7-i;j++){
                    System.out.print("   ");
                }
                for(int k=7;k>=7-i+1;k--){
                    System.out.print(a1);
                    a1++;
                }
                g--;
                 System.out.println();   
            }
        
    }
}




