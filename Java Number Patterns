public class Main
{
	public static void main(String[] args) {
            for(int i=1;i<=7;i++){
                    for(int k=1;k<=i;k++){
                        System.out.print("  ");
                    }
                    for(int j=i;j<=7;j++){
                    System.out.print(j);
                    }
                    
                    System.out.println();
            }
            
            for(int i=6;i>=1;i--){
                    for(int k=1;k<=i;k++){
                        System.out.print("  ");
                    }
                    for(int j=i;j<=7;j++){
                    System.out.print(j);
                    }
                    
                    System.out.println();
            }
        }
}

