public class Main
{
	public static void main(String[] args) {
        
        for(int i=1;i<=7;i++){
            for(int k=i;k<=7;k++){
                System.out.print(k+" ");
            }
            for(int j=1;j<i;j++){
                System.out.print(j+" ");
            }
        System.out.println();
        }
        }
}

