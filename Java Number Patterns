public class Main
{
	public static void main(String[] args) {
            for(int i=1;i<=7;i++){
              int  num=1;
               
             for(int j=1;j<=i;j++){
                System.out.print(num);
                
                 if(num==1){
                 num=0;
                } 
                else{
                 num=1;
                }
            }
            System.out.println();
            }
        }
}

