14.
*
* *
*   *
*     *
*       *
*         *
* * * * * * *

public class Main
{
	public static void main(String[] args) {
            for(int i=1;i<=7;i++){
                System.out.print("*");
            for(int k=2;k<=i;k++){
                    System.out.print("  ");
                }
                if(i==1 || i==7){
                    System.out.print(" ");
                }else   
                    System.out.print("* ");
            
                System.out.println();
                if(i==6){
                for(int j=1;j<=i;j++){
                System.out.print("* ");
                    
               
                }
                }
            }
        }
}
