//improvepublic class Main
{
	public static void main(String[] args) {

        for(int i=1;i<=6;i++){
            
            if(i==1){
                for(int j=1;j<=7;j++){
                    System.out.print("* ");
                }
            }
            System.out.println();
            
            System.out.print("* ");
            for(int k=1;k<=9-2*i;k++){ 
             
                System.out.print(" ");
            }
            
           if(i==7){
            System.out.print(" ");
           }
           else
            System.out.print("* ");
            
            
            //System.out.println();
            
        }
	}
}

