   * * * * *
    * * * *
     * * *
      * *
       *
   
public class Main
{
	public static void main(String[] args) {
            for(int i=1;i<=7;i++){
                for(int j=1;j<=7-i;j++){
                    System.out.print(" ");
                }
                for(int k=1;k<=i;k++){
                    System.out.print("* ");

                }
                System.out.println();
            }
            for(int i=1;i<=7;i++){
                for(int j=1;j<=i;j++){
                    System.out.print(" ");
                }
                for(int k=1;k<=7-i;k++){
                    System.out.print("* ");

                }
                System.out.println();
            }
	}
}
   
